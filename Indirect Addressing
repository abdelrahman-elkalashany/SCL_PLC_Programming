IF #Type = 'I' THEN
    #Memory_Type := 16#81;
ELSIF #Type = 'Q' THEN
    #Memory_Type := 16#82;
ELSIF #Type = 'M' THEN
    #Memory_Type := 16#83;
END_IF;

FOR #j := #Mem TO #Mem+#byte DO
    
    FOR #i := 0 TO 7 DO
        
        "Data_block_move_indirect_address".array[#i+#a] := PEEK_BOOL(area := #Memory_Type, dbNumber := 0, byteOffset := #j, bitOffset := #i);
        IF #i>=7 THEN
            #a += 8;
        END_IF;
        
    END_FOR;
    
END_FOR;
